# Container Classes
    .container
    .container-fluid

# Row Classes
    .row
    .row-cols-1 to .row-cols-12

# Column Classes
    .col
    .col-1 to col-12

    => Column Responsive Classes
    .col-xl-1 to .col-xl-12 (cover Extra large)
    .col-lg-1 to .col-lg-12 (cover Extra large and Large)
    .col-md-1 to .col-md-12 (cover Extra large and Large and Medium)
    .col-sm-1 to .col-sm-12 (cover Extra large and Large and Medium and Small)

# Color Classes
    .primary
    .secondary
    .success
    .light
    .warning
    .dark
    .danger

# Width Classes              # Height Classes
    .w-25                        .h-25
    .w-50                        .h-50
    .w-75                        .h-50
    .w-100                       .h-100

# Margin Classes
    .m  : Shorthand class (TRBL)
    .ms : margin start / margin left
    .me : margin end / margin right
    .my : margin about y-axis (top and bottom)
    .mx : margin about x-axis (left and right)
    .mt : margin top
    .mb : margin bottom
    .m-auto : margin auto

    => Units
        m-1 : 0.25rem (4px)
        m-2 : 0.5rem (8px)
        m-3 : 1rem (16px)
        m-4 : 1.5rem
        m-5 : 3rem

# Padding Classes
    .p  : Shorthand class (TRBL)
    .ps : padding start / padding left
    .pe : padding end / padding right
    .py : padding about y-axis (top and bottom)
    .px : padding about x-axis (left and right)
    .pt : padding top
    .pb : padding bottom
    .p-auto : padding auto


# Button Classes
    .btn
    .btn btn-danger
    .btn btn-warning
    .btn btn-success
    .btn btn-secondary
    .btn btn-primary
    .btn btn-light
    .btn btn-dark

# List Classes
    .list-group
    .list-group-item

# Table Classes
    .table 
    .table-bordered 
    .table-hover 
    .table-dark
    .table-striped

# Progress Bar Classes
    .progress
    .progress-bar 
    .progress-bar-striped 
    .progress-bar-animated

# Spinner Classes
    .spinner-border
    .spinner-grow